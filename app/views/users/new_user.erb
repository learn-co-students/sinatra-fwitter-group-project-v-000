<head>
  <title>Users</title>
</head>
 <h3>Sign Up</h3>
<% if !logged_in? %>
 <% unless locals.empty? %>
  <%= message %>
  <% end %>
<% end %>

 <br></br>
<form action="/signup" method="POST">
  <p>Name: <input type="text" name="username"></p>
  <p>Email: <input type="text" name="email"></p>
  <p>Password: <input type="text" name="password"></p>
  <input class="btn btn-primary" type="submit">
</form
