<h3>Here are your tweets, <%= @user.username %></h3>
<% @user.tweets.each do |tweet| %>
<blockquote class="blockquote">
  <p><a href="/tweets/<%= tweet.id %>"><%= tweet.id %>: </a><%= tweet.content %></p>
</blockquote>
<% end %>
<a class="btn btn-outline-success" role="button" href="/tweets/new">Tweet Here</a>
