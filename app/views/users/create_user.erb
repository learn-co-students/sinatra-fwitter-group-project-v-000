<!-- Users: Sign Up -->
<h1>Sign Up</h1>
<form action="/signup" method="post">
  <label>
    Name:
    <input type="text" name="username">
  </label>
  <br>
  <label>
    Email:
    <input type="text" name="email">
  </label>
  <br>
  <label>
    Password:
    <input type="text" name="password">
  </label>
  <br>
  <input type="submit" value="submit">
</form>
