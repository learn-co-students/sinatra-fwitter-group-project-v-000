<h2>Edit Tweet</h2>

<form action="/tweets/<%= @tweet.id %>" method="post">
  <input type="hidden" id="hidden" name="_method" value="patch">
  <label>Tweet:</label>
  <input type="text" name="content" value="<%= @tweet.content %>">

  <br/>
  <br/>

  <input type="submit" name="submit" value="submit">
</form>
