<div class="row">
  <div class="col-md-6">
    <p> Welcome, <%= current_user.username %>
    <h2> Tweets </h2>
  </div>
</div>
<div class="row">
  <% @tweets.each do |tweet| %>
    <div class="col-md-4 well">
      <a href="/tweets/<%=tweet.id%>"><%= tweet.content %></a>
    </div>
  <% end %>
</div>
