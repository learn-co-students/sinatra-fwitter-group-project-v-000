<h2>Welcome, <%=current_user.username %></h2>

<h2>Tweets</h2>
<% @tweets.each do |tweet| %>
<ul>
  <li> <%= tweet.user.username %></li>
  <ul>
    <li> <a href="/tweets/<%= tweet.id %>"><%= tweet.content %></a></li>
  </ul>
</ul>
  
<% end %>

<br><br>

<a href="tweets/new">Create a New Tweet</a>
<br>
<a href="/logout">Logout</a>
