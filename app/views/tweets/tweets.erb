<h1>Welcome, <%= current_user.username %>!</h1>

<h2><a href="/tweets/new">New Tweet!</a></h2>
<br>

<h2>Tweets</h2>

<% Tweet.all.each do |tweet| %>
  <p><%= tweet.content %> - <a href="/users/<%= tweet.user.slug %>"><%= tweet.user.username %></a> (<a href="/tweets/<%= tweet.id %>">view</a>)</p>

<% end %>
