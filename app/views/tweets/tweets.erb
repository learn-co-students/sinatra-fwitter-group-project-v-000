<h1>Welcome to Fwitter</h1>
<h3>Flatiron Twitter</h3>
<p>app/views/tweets/tweets.erb</p>
<a href="/">Home</a>
<br>
<a href="/logout">Log Out</a>
<br>
<a href="/tweets/new">Create a New Tweet</a>
<br>
<hr>
<h1>Welcome, <%= Helpers.current_user(session).username %>!</h1>
<h3>All your tweets:</h3>
<br>
<hr>
<% @tweets.each do | tweet | %>
  <%= tweet.content %>
  <br>
  <a href="/tweets/<%= tweet.id %>/edit">Edit Tweet</a>
  <br>
  <hr>
<% end %>
<hr>
