<h1>Welcome, <%= current_user.username %>.</h1>
<% @tweets.each do |tweet| %>
  <p><a href="/tweets/<%= tweet.id %>"><%= tweet.user.username %> tweeted:</p>
  <p><%= tweet.content %></a></p>
<% end if @tweets %>
<p><a href="/tweets/new">Create a new tweet!</a></p>
<p><a href="/logout">Log Out</a></p>
