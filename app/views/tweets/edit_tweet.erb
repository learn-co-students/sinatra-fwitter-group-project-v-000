<h1>Edit Tweet</h1>

<form action="/tweets/<%= @tweet.id %>" method="POST">
  <input type="hidden" name="_method" value="PATCH">

  <input type="text" name="title" value="<%= @tweet.title %>"<br>
  <input type="text" name="content" value="<%= @tweet.content %>"<br>
  <input type="submit" value="submit">
</form>
