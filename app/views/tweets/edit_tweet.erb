<h1>Edit Tweet</h1>

<form action="/tweets/<%= @tweet.id %>" method="POST">
  <input id="hidden" type="hidden" name="_method" value="PATCH">
    <textarea rows="5" column="50" name="content" id="content" value="<%= @tweet.content %>"></textarea>
    <br><br>
    <input type="submit" name="submit" value="Edit Tweet" id="submit">
</form>

