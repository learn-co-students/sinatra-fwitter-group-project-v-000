<h2>Edit tweet</h2>

<form action="/tweets/<%= @tweet.id %>" method="post">
    <input type="hidden" id="hidden" name="_method" value="patch">
<p>Tweet</p>
<input type="text" name="tweet[content]"value="<%= @tweet.content %>">

<p>Edit tweet here</p>
<textarea rows="10" cols="100" name="content" id="content" value=""></textarea>
<input type="submit" value="submit" id="submit">

</form>
