<h1>Edit Tweet #<%= @tweet.id %></h1><br>
<h2>Tweeter: <%= @current_user.username %></h2><br>
<form method="POST" action="/tweets/<%= @tweet.id %>">
  <input id="hidden" type="hidden" name="_method" value="PATCH">
  <label for="content">Content: </label><input type="text" name="content" id="content" value="<%= @tweet.content %>"><br>
  <input type="submit" value="submit">
</form>
<form method="POST" action="/tweets/<%= @tweet.id %>/delete">
  <input id="hidden" type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete Tweet"><br>
</form>
