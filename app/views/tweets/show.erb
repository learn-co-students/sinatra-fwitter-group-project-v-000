<h2><%= @tweet.user.username %>: <%= @tweet.content %></h2>

<a href="/tweets">Home</a>
<form class="" action="/tweets/<%= @tweet.id %>/edit" method="get">
  <input type="submit" value="Edit Tweet">
</form>
<form class="" action="/tweets/<%= @tweet.id %>/delete" method="post">
  <input type="submit" value="Delete Tweet">
</form>
