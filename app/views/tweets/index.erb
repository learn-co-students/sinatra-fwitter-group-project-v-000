<% if logged_in? %>
  <h2>Welcome, <%= current_user.username %> </h2>
  <% Tweet.all.each do |tweet| %>

      <%=tweet.content%>

  <%end%>
  <a href="/logout"> Log Out </a>
<% else %>
  <h4>Else show Sign Up or Login links</h4>

  <a href="/signup">Sign Up</a>

  <a href="/login"> Log In </a>
<%end %>
