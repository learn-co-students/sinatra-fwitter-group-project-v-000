<h1>Welcome, <%=current_user.username%></h1>
<a href="/tweets/new">Create New Tweet</a> <a href="/logout">Log Out</a>
<br>


<%@tweets.each do |tweet| %>
  <br>
  
  <p><%=tweet.content%></p>
  <p><a href="/tweets/<%=tweet.id%>.edit">Edit Tweet</a> <a href="/tweets/<%=tweet.id%>/delete">Delete Tweet</a></p>
  
  <br>
<%end%>
