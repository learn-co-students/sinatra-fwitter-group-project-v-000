<p>Welcome, <%= @user.username%>!</p>
<% @posts.each do |p| %>
  <% if p[:user_id] == session[:id] %>
  <br>
    <a href="users/<%= p.keys[0] %>"><%= p.keys[0] %></a>: <a href="tweets/<%= p[:id] %>/edit"><%= p.values[0] %></a>
  <% else %>
  <br>
    <a href="users/<%= p.keys[0] %>"><%= p.keys[0] %></a>: <%= p.values[0] %>
  <% end %>
<% end %>
