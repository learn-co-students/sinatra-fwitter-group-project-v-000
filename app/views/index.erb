<div class="jumbotron center">
  <h1>Welcome to Fwitter!</h1>
  <img src="./images/fwittericon.png" alt="Fwitter Icon"/><br>
  <% if logged_in? %>
    <h2>Welcome Back, <%=current_user.username %>!!</h2>
  <% else %>
    <a class="btn btn-primary" href="/signup">Sign Up</a>
    <a class="btn btn-default" href="/login">Log In</a>
  <% end %>
</div>
